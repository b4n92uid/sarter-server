    "date-fns": "^2.8.1",
    "dotenv": "^6.2.0",
    "express": "^4.17.1",
    "fast-glob": "^2.2.7",
    "fs-extra": "^9.0.1",
    "graphql": "^14.1.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.11",
    "lowdb": "^1.0.0",
    "mime-types": "^2.1.26",
    "mysql2": "^2.1.0",
    "mysqldump": "^3.0.0",
    "node-yaml": "^3.1.1",
