    "@types/debug": "0.0.31",
    "@types/dotenv": "^4.0.3",
    "@types/fs-extra": "^9.0.6",
    "@types/jest": "^24.0.13",
    "@types/lodash": "npm:@types/lodash-es",
    "@types/node": "^10.17.13",
    "@types/passport": "^1.0.2",
