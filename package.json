    "debug": "^4.1.1",
    "eslint": "^7.5.0",
    "hygen": "^6.0.4",
    "jest": "^24.8.0",
    "nodemon": "^1.18.10",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.2.0",
    "typescript": "^3.4.5"
  }
